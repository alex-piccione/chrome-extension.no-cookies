{
  "name": "Don't bother me about cookies!",
  "description": "Removes cookies settings annoying dialogs.",
  "version": "25.2.18",
  "manifest_version": 3,
  "icons": {
    "16": "/images/logo.png",
    "32": "/images/logo.png",
    "48": "/images/logo.png",
    "128": "/images/logo.png"
  },
  "permissions": ["activeTab", "scripting", "tabs"],
  "host_permissions": ["http://*/", "https://*/"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_icon": {
      "16": "/images/logo.png",
      "32": "/images/logo.png",
      "48": "/images/logo.png",
      "128": "/images/logo.png"
    }
  }
}
